{
	"name": "homescreen",
	"version": "0.1.0",
	"description": "A customisable home page for your web browser",
	"main": "webpack.config.js",
	"scripts": {
		"build": "npm run build-app && npm run build-proxy",
		"build-app": "webpack",
		"build-proxy": "tsc --outDir dist/proxy --project tsconfig.proxy.json",
		"start": "concurrently \"npm run start-app\" \"npm run start-proxy\" ",
		"start-app": "webpack --watch & live-server dist/app --port=8085",
		"start-proxy": "tsc --preserveWatchOutput --watch --outDir dist/proxy --project tsconfig.proxy.json & NODE_PATH=dist/proxy nodemon dist/proxy/server.js",
		"test": "false"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@gitlab.com/rubenwardy/homescreen.git"
	},
	"author": "rubenwardy",
	"license": "MIT",
	"bugs": {
		"url": "https://gitlab.com/rubenwardy/homescreen/issues"
	},
	"homepage": "https://gitlab.com/rubenwardy/homescreen#readme",
	"dependencies": {
		"@types/express": "^4.17.11",
		"@types/node-fetch": "^2.5.8",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.1",
		"copy-webpack-plugin": "^7.0.0",
		"css-loader": "^5.1.1",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"html-webpack-plugin": "^5.2.0",
		"mini-css-extract-plugin": "^1.3.9",
		"node-fetch": "^2.6.1",
		"node-sass": "^5.0.0",
		"nodemon": "^2.0.7",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"sass-loader": "^11.0.1",
		"ts-loader": "^8.0.17",
		"typescript": "^4.2.2",
		"webpack": "^5.24.2",
		"webpack-cli": "^4.5.0"
	},
	"devDependencies": {
		"concurrently": "^6.0.0",
		"live-server": "^1.2.1"
	}
}
