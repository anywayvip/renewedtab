{
	"manifest_version": 2,
	"name": "Renewed Tab",
	"version": "1.6.0",
	"description": "A customisable New Tab page, with widgets and beautiful backgrounds",
	"author": "rubenwardy",
	"homepage_url": "https://renewedtab.com/",
	"icons": {
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"chrome_url_overrides" : {
		"newtab": "app/webext.html"
	},
	"chrome_settings_overrides": {
		"homepage": "app/webext.html"
	},
	"permissions": [
		"storage",
		"search"
	],
	"optional_permissions": [
		"<all_urls>",
		"bookmarks",
		"topSites"
	]
}
