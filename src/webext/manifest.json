{
	"manifest_version": 2,
	"name": "Renewed Tab",
	"version": "1.13.1",
	"description": "A customisable New Tab page, with widgets and beautiful backgrounds",
	"author": "rubenwardy",
	"homepage_url": "https://renewedtab.com/",
	"offline_enabled": true,
	"minimum_chrome_version": "78",
	"icons": {
		"32": "icon-32.png",
		"48": "icon-48.png",
		"128": "icon-128.png"
	},
	"chrome_url_overrides" : {
		"newtab": "app/webext.html"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "{166411f2-402a-4bca-a3da-38b795ec8007}",
			"strict_min_version": "78.0"
		}
	},
	"permissions": [
		"storage",
		"search"
	],
	"optional_permissions": [
		"<all_urls>",
		"bookmarks",
		"topSites"
	]
}
